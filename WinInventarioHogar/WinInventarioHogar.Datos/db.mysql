CREATE DEFINER=`root`@`localhost` PROCEDURE `SP_UserLogin`(in username varchar(50), in password varchar(150))
BEGIN
	select u.id, u.name nameUser, u.username, u.idRoll, r.name nameRoll
    from user u, roll r
    where u.idRoll = r.id
    and lower(u.username) = lower(username)
    and u.password = md5(password);
END ;;
