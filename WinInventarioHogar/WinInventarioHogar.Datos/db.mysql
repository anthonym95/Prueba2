CREATE DEFINER=`root`@`localhost` PROCEDURE `SP_UserUpdate`(
	in id int,
    in name varchar(255),
    in username varchar(50),
    in password varchar(50),
    in idRoll int
)
BEGIN
	update user u
    set u.name = name,
		u.username = username,
        u.idRoll = idRoll,
        u.updated_at = now(),
        u.password = case when password = '' then u.password else md5(password) end
	where u.id = id;
END ;;
